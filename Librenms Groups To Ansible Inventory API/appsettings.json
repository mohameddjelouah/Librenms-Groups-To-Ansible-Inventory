{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth_Type": "X-Auth-Token", // this is the auth type to acces librenms api
  "Token": "Your Generated Token From LibreNms", // generate a token from librenms to use for auth
  "Url": "http://YourLibrenmsURL/api/v0", // your librenms url
  "Dir": "Path for saving the inventory file" //Put here your path for the inventory example : linux "/etc/ansible/inventory.txt" windows "C:\\apps\\inventory.txt"
}
